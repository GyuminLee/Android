<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/startActivityIfNeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startActivityIfNeed" />

    <Button
        android:id="@+id/startNextMatching"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startNextMatchingActivity" />

    <Button
        android:id="@+id/startInentSender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startIntentSender" />

    <Button
        android:id="@+id/startIntentSenderForResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startIntentSenderForResult" />

</LinearLayout>